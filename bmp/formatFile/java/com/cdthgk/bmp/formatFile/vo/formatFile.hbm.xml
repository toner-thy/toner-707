<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cdthgk.bmp.formatFile.vo.FormatFile" table="bip_format_file">
        <id name="id" type="java.lang.String">
            <column name="id" length="39" />
            <generator class="com.cdthgk.persistece.hibernate.idgen.AssignedUUIDGenerator" />
        </id>
        <property name="name" type="java.lang.String">
			<column name="name" >
			</column>
		</property>
        <property name="fileUrl" type="java.lang.String">
			<column name="file_url" >
			</column>
		</property>
        <property name="formatFileUrl" type="java.lang.String">
			<column name="format_file_url" >
			</column>
		</property>
		<many-to-one name="organ" class="com.cdthgk.platform.organization.organ.domain.Organ">
			<column name="organ_id">
			</column>
		</many-to-one>
		<many-to-one name="createPerson" class="com.cdthgk.platform.permission.user.domain.User">
			<column name="CREATE_PERSON">
				<comment>创建人</comment>
			</column>
		</many-to-one>
		<property name="createTime" type="java.util.Date">
			<column name="CREATE_TIME" >
				<comment>创建时间</comment>
			</column>
		</property>
		<many-to-one name="modifyPerson" class="com.cdthgk.platform.permission.user.domain.User">
			<column name="MODIFY_PERSON" >
				<comment>修改人</comment>
			</column>
		</many-to-one>
		<property name="modifyTime" type="java.util.Date">
			<column name="MODIFY_TIME" >
				<comment>修改时间</comment>
			</column>
		</property>
    </class>
</hibernate-mapping>
